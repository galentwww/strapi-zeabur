{
  "kind": "collectionType",
  "collectionName": "teachers",
  "info": {
    "singularName": "teacher",
    "pluralName": "teachers",
    "displayName": "teacher"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "grades": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::grade.grade",
      "inversedBy": "teachers"
    },
    "subjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subject.subject",
      "inversedBy": "teachers"
    },
    "registerDate": {
      "type": "date"
    },
    "teacherStatus": {
      "type": "enumeration",
      "required": true,
      "default": "active",
      "enum": [
        "active",
        "inactive",
        "suspended",
        "resigned"
      ]
    },
    "schedules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::schedule.schedule",
      "mappedBy": "teacher"
    },
    "class_groups": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::class-group.class-group",
      "mappedBy": "teacher"
    }
  }
}
